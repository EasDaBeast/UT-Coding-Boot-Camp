<div class="row">
    <div class="col s12 m10 offset-m1">
        <div class="row">
            <div class="col s12 m10 offset-m1">
                <h1>Latest Tech News</h1>

                <ul>
                    {{#each threads}}
                    <li>
                        <a href="/showthread_{{this.threadId}}" target="_blank">{{this.title}}</a> by {{this.author}}
                    </li>
                    {{/each}}
                </ul>
            </div>
        </div>
    </div>
</div>